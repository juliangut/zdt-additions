<?php /* @var $collector \DoctrineORMModule\Collector\MappingCollector */ ?>
<div class="zdt-toolbar-entry">
    <div class="zdt-toolbar-preview">
        <img src="data:image/png;base64," alt="Request">
        <span class="zdt-toolbar-info">
            Request
        </span>
    </div>
    <div class="zdt-toolbar-detail">
        <?php
        $collectors = array(
            'Get'    => $this->collector->getGet(),
            'Post'   => $this->collector->getPost(),
            'Cookie' => $this->collector->getCookie(),
            'File'   => $this->collector->getFile(),
        );

        foreach ($collectors as $collector => $collection) {
            if ($collection) { ?>
            <span class="zdt-toolbar-info">
                <span class="zdt-detail-label"><?php echo $collector ?></span>
                    <span class="zdt-detail-value">
                    <pre><?php
                    foreach ($collection as $key => $val) {
                        echo $key . ': ' . $val . "\n";
                    }
                    ?>
                    </pre>
                </span>
            </span>
            <?php
            }
        }
        ?>
    </div>
</div>
