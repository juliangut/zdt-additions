{
    "name": "juliangut/zdt-additions",
    "type": "zf2-module",
    "description": "Zend Developer Tools additional collectors.",
    "keywords": [
        "zf2",
        "zdt",
        "request"
    ],
    "homepage": "https://github.com/juliangut/zdt-additions",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Julián Gutiérrez",
            "email": "juliangut@gmail.com",
            "homepage": "http://juliangut.com",
            "role": "lead"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-developer-tools": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "JgutZdtAdditions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    }
}
